<?xml version="1.0" ?>
<odoo>
    <record id="account_bank_statement_line_tree" model="ir.ui.view">
        <field name="model">account.bank.statement.line</field>
        <field name="inherit_id" ref="account_statement_base.account_bank_statement_line_tree"/>
        <field name="arch" type="xml">
            <field name="partner_id" position="before">
                <field name="partner_name" optional="show" string="İş Ortağı Bilgisi"/>
            </field>
            <field name="narration" position="attributes">
                <attribute name="optional">show</attribute>
            </field>
        </field>
    </record>
</odoo>