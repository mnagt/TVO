<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="state_deposit" model="cheque.state.option">
            <field name="name">Deposited</field>
            <field name="code">deposit</field>
            <field name="sequence">10</field>
        </record>

        <record id="state_bounce" model="cheque.state.option">
            <field name="name">Bounced</field>
            <field name="code">bounce</field>
            <field name="sequence">20</field>
        </record>

        <record id="state_return" model="cheque.state.option">
            <field name="name">Returned</field>
            <field name="code">return</field>
            <field name="sequence">30</field>
        </record>

        <record id="state_done" model="cheque.state.option">
            <field name="name">Cashed</field>
            <field name="code">done</field>
            <field name="sequence">40</field>
        </record>

    </data>
</odoo>
